{
  "manifest_version": 2,
  "name": "kde-activities-z",
  "description": "Auto move new firefox tabs to the current kde activities.",
  "version": "1.3",
  "icons": {
    "64": "icons/iconz.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/iconz.png"
    },
    "default_title": "kde-activities-z"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "kde-activities-z@david.xyz",
      "strict_min_version": "55.0"
    }
  }
}
